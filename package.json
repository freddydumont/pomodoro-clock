{
  "name": "pomodoro-clock",
  "homepage": "https://freddydumont.github.io/pomodoro-clock",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "howler": "^2.0.4",
    "include-media": "^1.4.9",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/style/App.scss -o src/style",
    "watch-css": "npm run build-css && node-sass-chokidar src/style/App.scss -o src/style --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
